@model DemoApp.ViewModels.HomeViewModel
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-2">
       
    </div>
    <div class="col-md-8">
        <table class="table table-bordered table-striped" >
            <tr>
                <td>Name</td>
                <td>Value</td>
            </tr>
            <tr>
                <td>First Name</td>
                <td>@Model.User.FirstName</td>
            </tr>
            <tr>
                <td>Last Name</td>
                <td>@Model.User.LastName</td>
            </tr>
            <tr>
                <td>UPN</td>
                <td>@Model.User.UPN</td>
            </tr>
            <tr>
                <td>AD User ID</td>
                <td>@Model.User.ADUserName</td>
            </tr>
            <tr>
                <td>BCM ID</td>
                <td>@Model.User.BCMID</td>
            </tr>
            <tr>
                <td>Email</td>
                <td>@Model.User.Email</td>
            </tr>
        </table>
    </div>
    <div class="col-md-2">

    </div>
</div>